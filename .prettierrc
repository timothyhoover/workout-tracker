{
  "singleQuote": true,
  "semi": false,
  "arrowParens": "always",
  "trailingComma": "all",
  "endOfLine": "auto",
  "closingBracketPosition": "last-line",
  "vueIndentScriptAndStyle": true,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "tabWidth": 2,
  "useTabs": false
}
